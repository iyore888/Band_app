<% provide(:title, "Users")%>

<h1 class="h1s">Users Index</h1>

<p>ユーザー検索</p>
  <p>
    <%= form_tag users_path, method: :get, class:"search"  do %>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag 'Search', name: :nil %>
    <% end %>
  </p>
  
<%= will_paginate %>

<ul class="users">
    <%= render @users %>
</ul>

<%= will_paginate %>